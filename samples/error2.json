    "resources" : [
        {
            "name" : "Order",
            "path" : "/{orderId}",
            "operations" : [
                {
                    "name" : "deleteOrder",
                    "method":"DELETE",
                    "description":"Remove the order identified by the specified ID from the system",
                    "input":{
                        "params":[
                            {
                                "optional":false,
                                "binding":"orderIdBinding"
                            }
                        ]
                    },
                    "output":{
                        "type":"Order",
                        "contentType":["application/json"],
                        "status":200
                    },
                    "errors":[
                        {
                            "cause":"Specified order does not exist",
                            "status":404
                        },
                        {
                            "cause":"An unexpected runtime exception",
                            "status":500
                        }
                    ]
                },
                {
                    "name" : "getOrder",
                    "method":"GET",
                    "description":"Retrieve the order identified by the specified identifier",
                    "input":{
                        "params":[
                            {
                                "optional":false,
                                "binding":"orderIdBinding"
                            }
                        ]
                    },
                    "output":{
                        "type":"Order",
                        "contentType":["application/json"],
                        "status":200
                    },
                    "errors":[
                        {
                            "cause":"Specified order does not exist",
                            "status":404
                        },
                        {
                            "cause":"An unexpected runtime exception",
                            "status":500
                        }
                    ]
                }
            ]
        },
        {
            "name" : "AllOrders",
            "path" : "/",
            "operations" : [
                {
                    "name" : "getAllOrders",
                    "method" : "GET",
                    "output" : {
                        "status" : 200,
                        "contentType" : [ "json" ],
                        "type" : "list(Order)"
                    }
                },
                {
                    "name" : "submitOrder",
                    "method" : "POST",
                    "input" : {
                        "contentType" : "json",
                        "type" : "OrderRequest"
                    },
                    "output" : {
                        "status" : 201,
                        "contentType" : [ "json" ],
                        "type" : "Order"
                    }
                }
            ]
        }
    ],
    "dataTypes" : [
        {
            "name":"Order",
            "fields":[
                {
                    "name":"orderId",
                    "type":"string",
                    "description":"Unique system generated string identifier of the drink.",
                    "optional":false
                },
                {
                    "name":"drink",
                    "type":"string",
                    "description":"Name of the drink",
                    "optional":false
                },
                {
                    "name":"additions",
                    "type":"list(string)",
                    "description":"List of additions (flavors) to be included in the drink",
                    "optional":false
                },
                {
                    "name":"cost",
                    "type":"double",
                    "description":"Cost of the drink in USD",
                    "optional":false
                },
                {
                    "name":"next",
                    "type":"href",
                    "ref":"Order",
                    "description":"A URL pointing to the next resource in the workflow",
                    "optional":false
                }
            ]
        },
        {

            "name":"OrderRequest",
            "fields":[
                {
                    "name":"drink",
                    "type":"string",
                    "description":"Name of the drink to order",
                    "optional":false
                },
                {
                    "name":"additions",
                    "type":"list(string)",
                    "description":"A list of additions to be included in the drink",
                    "optional":true
                }
            ]
        }
    ],
    "sla":[
        {
            "name":"FREE",
            "availability":99.7,
            "rateLimit":20,
            "timeUnit":"second"
        },
        {
            "name":"SILVER",
            "availability":99.8,
            "rateLimit":250,
            "timeUnit":"second",
            "costModel":{
                "currency":"USD",
                "unitPrice":0.05,
                "requestsPerUnit":1000
            }
        },
        {
            "name":"GOLD",
            "availability":99.9,
            "rateLimit":1000,
            "timeUnit":"second",
            "costModel":{
                "currency":"USD",
                "unitPrice":0.1,
                "requestsPerUnit":1000
            }
        }
    ]
}